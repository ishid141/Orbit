<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".resourcepage"
    android:background="@drawable/bluebg"
    >

    <ImageView
        android:id="@+id/resources"
        android:layout_width="287dp"
        android:layout_height="75dp"
        android:elevation="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/resourcesimage" />

    <View
        android:id="@+id/middledivider"
        android:layout_width="437dp"
        android:layout_height="1dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.516" />

    <View
        android:id="@+id/divider5"
        android:layout_width="1dp"
        android:layout_height="818dp"
        android:background="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resources" />

    <Button
        android:id="@+id/math"
        android:layout_width="132dp"
        android:layout_height="40dp"
        android:layout_marginEnd="264dp"
        android:text="Math"
        android:fontFamily="@font/scmedium"
        android:backgroundTint="@color/cyan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.101" />

    <Button
        android:id="@+id/science"
        android:layout_width="132dp"
        android:layout_height="40dp"
        android:text="Science"
        android:fontFamily="@font/scmedium"
        android:backgroundTint="@color/cyan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.843"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.101" />

    <Button
        android:id="@+id/history"
        android:layout_width="132dp"
        android:layout_height="40dp"
        android:backgroundTint="@color/cyan"
        android:text="History"
        android:fontFamily="@font/scmedium"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.133"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.544" />

    <Button
        android:id="@+id/english"
        android:layout_width="132dp"
        android:layout_height="40dp"
        android:backgroundTint="@color/cyan"
        android:text="English"
        android:fontFamily="@font/scmedium"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.846"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.544" />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="179dp"
        android:layout_height="104dp"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="243dp"
        android:fontFamily="@font/scmedium"
        android:text="*Organic Chemistry Tutor"
        android:textColor="@color/white"
        android:textSize="19dp"
        android:autoLink="web"
        android:onClick="openLink"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.176"
        />


</androidx.constraintlayout.widget.ConstraintLayout>